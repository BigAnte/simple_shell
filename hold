	if (command_type == INTERNAL_COMMAND_1)
	{
		/* build array of environment variables */
		for (i = 0; environ[i]; i++)
			new_env[i] = _strdup(environ[i]);
		new_env[i] = NULL;
		_cd(tokenized_command, new_env);
	}

    /** my code */
	if (_strcmp(command, internal_command[2]))
		return (INTERNAL_COMMAND_1);